CREATE TABLE ALUNO (
    ID INT serial PRIMARY KEY,
    nome VARCHAR(255),
    email VARCHAR(255),
    endereco VARCHAR(255)
 );
 
CREATE TABLE EMPRESTIMO (
  ID INT serial PRIMARY KEY,
  CODIGO INT NOT NULL,
  QUANDO DATE,
  MATRIC_ALUNO INT NOT NULL,
  DEVOLUCAO DATE
  );
  
